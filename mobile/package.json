  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "lint": "prettier --write '{src,test}/**/*.{js,jsx}' && eslint ./src",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "WEB_PORT=3000 expo start --web",
    "eject": "expo eject",
    "storybook": "start-storybook --ci",
    "prestorybook": "rnstl --searchDir ./src --pattern \"**/*.stories.jsx\" --outputFile ./storybook/storyLoader.js",
    "relay": "yarn run relay-compiler --schema src/relay/schema.graphql --artifactDirectory ./src/__generated__ --src ./src/ --extensions js jsx --watchman false $@",
    "relay-watch": "yarn run relay-compiler --schema src/relay/schema.graphql --artifactDirectory ./src/__generated__ --src ./src/ --extensions js jsx --watch",
    "schema": "get-graphql-schema http://127.0.0.1:8000/schema/ > src/relay/schema.graphql",
    "generate": "yarn schema",
    "build": "next build"
  },
